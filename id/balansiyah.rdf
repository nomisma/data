        <dcterms:isPartOf rdf:resource="http://nomisma.org/id/medieval_spanish_numismatics"/>
        <nmo:hasEndDate rdf:datatype="http://www.w3.org/2001/XMLSchema#gYear">1238</nmo:hasEndDate>
        <skos:closeMatch rdf:resource="https://www.wikidata.org/wiki/Q11907651"/>
