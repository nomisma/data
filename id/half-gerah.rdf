   <nmo:Denomination rdf:about="http://nomisma.org/id/half-gerah">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:definition xml:lang="en">Denomination of the Judahite Standard most likely based on a sheqel of 9.60g. Mean weight of 0.25g. The name derives from the epigraphically attested Late Iron Age II/early Yehud weight unit gerah.</skos:definition>
        <skos:prefLabel xml:lang="en">1/2 gerah</skos:prefLabel>
        <skos:definition xml:lang="fr">1/2 gerrah</skos:definition>
        <skos:prefLabel xml:lang="fr">1/2 gerrah</skos:prefLabel>
        <skos:prefLabel xml:lang="es">1/2 agorah</skos:prefLabel>
