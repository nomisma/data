        <skos:prefLabel xml:lang="en">Tinos</skos:prefLabel>
        <skos:prefLabel xml:lang="de">Tinos</skos:prefLabel>
        <skos:prefLabel xml:lang="fr">Tinos</skos:prefLabel>
