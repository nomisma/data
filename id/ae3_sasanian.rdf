    <nmo:Denomination rdf:about="http://nomisma.org/id/ae3_sasanian">
        <skos:prefLabel xml:lang="en">AE 3</skos:prefLabel>
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:definition xml:lang="en">Bronze denomination issued in the Sasanian Empire during the reigns up to Ohrmazd II as defined by Sylloge Nummorum Sasanidarum I (2003) p. 116 for the reign of Ardashir I (weight  c. 2 grams).
For kings from Shapur II no such definition seems currently to exist: Sylloge Nummorum Sasanidarum III (2004) pp. 116-117. </skos:definition>
        <skos:inScheme rdf:resource="http://nomisma.org/id/"/>
        <skos:changeNote rdf:resource="http://nomisma.org/id/ae3_sasanian#provenance"/>
        <dcterms:isPartOf rdf:resource="http://nomisma.org/id/sasanian_numismatics"/>
        <skos:exactMatch rdf:resource="https://ikmk.smb.museum/ndp/denomination/2680"/>
    </nmo:Denomination>
