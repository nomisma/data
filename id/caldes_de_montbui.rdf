        <skos:prefLabel xml:lang="en">Caldes de Montbui</skos:prefLabel>
        <skos:prefLabel xml:lang="es">Caldas de Montbuy</skos:prefLabel>
        <skos:prefLabel xml:lang="ca">Caldes de Montbui</skos:prefLabel>
