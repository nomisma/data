        <dcterms:isPartOf rdf:resource="http://nomisma.org/id/medieval_european_numismatics"/>
        <nmo:hasEndDate rdf:datatype="http://www.w3.org/2001/XMLSchema#gYear">1226</nmo:hasEndDate>
        <geo:location rdf:resource="http://nomisma.org/id/alt-toggenburg#this"/>
    </nmo:Mint>
