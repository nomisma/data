        <rdfs:subPropertyOf rdf:resource="http://nomisma.org/ontology#hasMeasurement"/>
        <rdfs:comment xml:lang="en">Describes the depth of an object.</rdfs:comment>
        <rdfs:label>hasDepth</rdfs:label>
