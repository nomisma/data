    </rdf:Property>
    


    <!-- http://nomisma.org/ontology#hasIconography -->

    <rdf:Property rdf:about="http://nomisma.org/ontology#hasIconography">
        <rdfs:subPropertyOf rdf:resource="http://nomisma.org/ontology#hasAppearance"/>
